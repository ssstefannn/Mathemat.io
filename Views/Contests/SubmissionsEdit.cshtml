@model Mathemat.io.Models.ContestSubmissions

@{
    ViewData["Title"] = "SubmissionsEdit";
}

<h1>SubmissionsEdit</h1>

<h4>ContestSubmissions</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="SubmissionsEdit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ContestID" />
            <input type="hidden" asp-for="ProblemID" />
            <input type="hidden" asp-for="ContestantID" />
            <div class="form-group">
                <label asp-for="Solution" class="control-label"></label>
                <input asp-for="Solution" class="form-control" />
                <span asp-validation-for="Solution" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Points" class="control-label"></label>
                <input asp-for="Points" class="form-control" />
                <span asp-validation-for="Points" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

